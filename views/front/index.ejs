<% layout("layout") %>

<!-- section -->
<div class="tp-banner-container">
	<img src="../../../front/kapak2.jpeg" style="width: 100%; height: auto; object-fit: cover;" >
</div>
<!-- / revolution slider -->
<hr class="divider-color">
<!-- content -->
<section class="padding-section">
	<div class="grid-row clear-fix">
		<h2 class="center-text">Eğitim Setlerimiz</h2>
		<div class="grid-col-row">
			<% set.forEach(element => { %>
				<div class="grid-col grid-col-3">
					<!-- course item -->
					<div class="course-item">
						<div class="course-hover">
							<img src="<%= element.kapak %>" alt>
							<div class="hover-bg bg-color-1"></div>
							<a href="/set/<%= element._id %>">Satın Al</a>
						</div>
						<div class="course-name clear-fix">
							<span class="price"> <b><%= element.fiyat %></b> ₺ </span>
						<h3><a href="#"><%= element.title %></a></h3>
							</div>
					</div>
				</div>
			<% }) %>
		</div>
	</div>
</section>
<hr class="divider-color" />
<section class="padding-section">
	<div class="grid-row clear-fix">
		<h2 class="center-text">Kitaplarımız</h2>
		<div class="grid-col-row">
			<% kitap.forEach(element => { %>
				<div class="grid-col grid-col-3">
					<!-- course item -->
					<div class="course-item">
						<div class="course-hover">
							<img src="<%= element.kapak %>" style="object-fit: cover;">
							<div class="hover-bg bg-color-1"></div>
							<a href="/kitap/<%= element._id %>">İNCELE</a>
						</div>
						<div class="course-name clear-fix">
							<span class="price"> <b><%= element.price-10%> ₺</b> </span>
						<h3><a href="#"><%= element.title %></a></h3>
							</div>
					</div>
				</div>
			<% }) %>
		</div>
	</div>
</section>
<hr class="divider-color" />
<section class="padding-section">
	<div class="grid-row clear-fix">
		<h2 class="center-text">Çözüm Videoları</h2>
		<div class="grid-col-row">
			<% video.forEach(element => { %>
				<div class="grid-col grid-col-3">
					<!-- course item -->
					<div class="course-item">
						<div class="course-hover">
							<img src="<%= element.kapak %>" style="object-fit: cover;">
							<div class="hover-bg bg-color-1"></div>
							<a href="/video/<%= element._id %>">Satın Al</a>
						</div>
						<div class="course-name clear-fix">
							<span class="price"> <b><%= element.fiyat %></b> ₺ </span>
						<h3><a href="/video/<%= element._id %>"><%= element.title %> </a></h3>
							</div>
					</div>
				</div>
			<% }) %>
		</div>
	</div>
</section>
<hr class="divider-color" />
<!-- paralax section -->
<div class="parallaxed">
	<div class="parallax-image" data-parallax-left="0.5" data-parallax-top="0.3" data-parallax-scroll-speed="0.5">
		<img src="../../../front/img/parallax.png" alt="">

	</div>
	<div class="them-mask bg-color-3"></div>
	<div class="grid-row">
		<div class="grid-col-row clear-fix">
			<div class="grid-col grid-col-4 alt">
				<div class="counter-block">
					<i class="flaticon-camera"></i>
					<div class="counter" data-count="656">0</div>
					<div class="counter-name">Video Eğitim</div>
				</div>
			</div>
			<div class="grid-col grid-col-4 alt">
				<div class="counter-block">
					<i class="flaticon-book1"></i>
					<div class="counter" data-count="5">0</div>
					<div class="counter-name">Kitap Destekli</div>
				</div>							
			</div>
			<div class="grid-col grid-col-4 alt">
				<div class="counter-block">
					<i class="flaticon-users"></i>
					<div class="counter" data-count="1813">0</div>
					<div class="counter-name">Kullanıcı Sayısı</div>
				</div>
			</div>
		</div>
	</div>
</div>
<!-- / paralax section -->
